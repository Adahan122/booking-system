# Example environment variables for deployment
# Copy to .env (locally) or set in Render dashboard
SECRET_KEY=change-me-to-a-secure-value
# DATABASE_URL example for local Postgres (Render will provide its own)
# DATABASE_URL=postgres://user:password@host:port/dbname
MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_USE_TLS=True
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-email-password
MAIL_DEFAULT_SENDER=noreply@booking-system.com
ADMINS=admin@booking-system.com